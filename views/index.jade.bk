doctype html
html
  head
    title  CCTNS Dashboard
    link(rel='stylesheet', type='text/css', href='/stylesheets/style.css')
  body
    table#dropdown-panel
      tr
        td.dropdown-menu
          p जोन
          form.drop-form
            select#s0
              option All
        td.dropdown-menu
          p परिक्षेत्र
          form.drop-form
            select#s1
              option All
        td.dropdown-menu
          p जनपद
          form.drop-form
            select#s2
              option All
        td.dropdown-menu
          p थाना
          form.drop-form
            select#s3
              option All
