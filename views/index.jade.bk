      tr
        td#plot1.plots
        td#plot2.plots
      tr
        td.footers
          p अपराध सम्बन्धी
        td.footers
          p नागरिक सेवा सम्बन्धी

    script(src='http://d3js.org/d3.v3.min.js')
    script(src='http://labratrevenge.com/d3-tip/javascripts/d3.tip.v0.6.3.js')
    script(type='text/javascript', src='/javascripts/script.js')
